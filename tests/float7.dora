fun main() {
    let array = FloatArray();
    assert(0 == array.len());

    let array = FloatArray(10, 2F);
    assert(10 == array.len());
    assert(2F == array[0]);
    assert(2F == array[9]);

    let array = DoubleArray();
    assert(0 == array.len());

    let array = DoubleArray(12, 3D);
    assert(12 == array.len());
    assert(3D == array[0]);
    assert(3D == array[11]);
}