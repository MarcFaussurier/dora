//= vm-args "--gc-young-ratio=2 --max-heap-size=128M"

class MyThread() : Thread {
    override fun run() {
        allocator();
    }
}

fun main() {
    let thread = MyThread();
    thread.start();

    allocator();
}

fun allocator() {
    var i = 0;

    while i < 10_000 {
        Foo();
        i = i + 1;
    }
}

class Foo