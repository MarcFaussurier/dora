class Interval {
    let number: int;
    let width: int;
    let height: int;
    let yfrom: int;
    let yto: int;
    let total: int;
    let threadid: int;

    init(number: int, width: int, height: int, yfrom: int, yto: int, total: int, threadid: int) {
        self.number = number;
        self.width = width;
        self.height = height;
        self.yfrom = yfrom;
        self.yto = yto;
        self.total = total;
        self.threadid = threadid;
    }
}